class Solution
{
    public String is_palindrome(int n)
    {
        // Code here
        int pal = n;
        int rev = 0;
        
        while(n > 0){
            int ld = n % 10;
            rev = rev * 10 + ld;
            n = n / 10;
        }
        if(rev == pal){
            return "Yes";
        }else{
            return "No";
        }
    }
}
