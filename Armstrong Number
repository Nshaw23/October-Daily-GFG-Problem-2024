class Solution {
    static String armstrongNumber(int n) {
        // code here
        int arms = n;
        int sum = 0;
        while(n > 0){
            int ld = n % 10;
            sum = sum + ld * ld * ld;
            n = n / 10;
        }
        if(arms == sum){
            return "true";
        }else{
            return "false";
        }
    }
}
